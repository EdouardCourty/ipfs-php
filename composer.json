{
    "name": "ecourty/ipfs-php",
    "description": "A lightweight IPFS interaction library for PHP",
    "type": "library",
    "require": {
        "php": ">= 8.3",
        "symfony/http-client": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.71",
        "symfony/var-dumper": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ipfs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ipfs\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edouard Courty",
            "email": "edouard.courty2@gmail.com",
            "homepage": "https://github.com/EdouardCourty"
        }
    ]
}
